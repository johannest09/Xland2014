    <section name="dotless" type="dotless.Core.configuration.DotlessConfigurationSectionHandler, dotless.Core" />
    <section name="resizer" type="ImageResizer.ResizerSection,ImageResizer" requirePermission="false" />
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <resizer>
    <clientcache minutes="0|1440" />
    <presets onlyAllowPresets="true|false">
      <preset name="thumb-defs" defaults="width=100;height=100" />
      <preset name="thumb" settings="width=100;height=100" />
      <preset name="thumb-width" defaults="height=100" settings="width=100" />
    </presets>
    <!-- The height can be overriden above, but not the width -->
    <diagnostics enableFor="Localhost|AllHosts|None" />
    <diskCache dir="~/imagecache" autoClean="false" hashModifiedDate="true" enabled="true" subfolders="32" cacheAccessTimeout="15000" />
    <cleanupStrategy startupDelay="00:05" minDelay="00:00:20" maxDelay="00:05" optimalWorkSegmentLength="00:00:04" targetItemsPerFolder="400" maximumItemsPerFolder="1000" avoidRemovalIfCreatedWithin="24:00" avoidRemovalIfUsedWithin="4.00:00" prohibitRemovalIfUsedWithin="00:05" prohibitRemovalIfCreatedWithin="00:10" />
    <plugins>
      <add name="DefaultEncoder" />
      <add name="NoCache" />
      <add name="ClientCache" />
      <add name="Diagnostics" />
      <add name="FolderResizeSyntax" />
      <add name="MvcRoutingShim" />
      <add name="IEPngFix" redirect="false" catchAll="false" />
      <add name="SizeLimiting" />
      <add name="SpeedOrQuality" />
      <add name="DropShadow" />
      <add name="Presets" />
      <add name="Logging" />
      <add name="ImageHandlerSyntax" />
      <add name="Image404" />
      <add name="Gradient" />
      <add name="DefaultSettings" />
      <add name="VirtualFolder" virtualPath="~/Images" physicalPath="/Images" />
      <add name="AutoRotate" />
      <add name="RemoteReader" />
      <add name="CloudFront" />
      <diskcache enabled="false" />
      <add name="SourceMemCache" />
      <add name="MemCache" />
      <add name="WicDecoder" />
      <add name="WicEncoder" />
      <add name="WicBuilder" />
      <add name="Watermark" />
      <add name="SeamCarving" />
      <add name="FreeImageDecoder" />
      <add name="FreeImageEncoder" />
      <add name="FreeImageBuilder" />
      <add name="FreeImageResizer" />
      <add name="AdvancedFilters" />
      <add name="SimpleFilters" />
      <add name="WhitespaceTrimmer" />
    </plugins>
  </resizer>
  <connectionStrings>
    <add name="XlandContext" connectionString="Server=tcp:jebk25rgp2.database.windows.net,1433;Database=xland;User ID=xlandAdmin@jebk25rgp2;Password=SuperchampXD1;Trusted_Connection=False;Encrypt=True;Connection Timeout=30; MultipleActiveResultSets=true" providerName="System.Data.SqlClient" />
